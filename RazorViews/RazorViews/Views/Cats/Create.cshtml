@model CatViewModel

@{
    ViewData["Title"] = "Create";
}

<form method="post">
    <div class="form-group">
        <label asp-for="Name"/>
        <input asp-for="Name" class="form-control" placeholder="Enter cat's name..">
    </div>
    <div class="form-group">
        <label asp-for="Age"/>
        <input asp-for="Age" type="number" class="form-control" placeholder="Enter cat's age..">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>


<form  method="post">

@Html.LabelFor(c => c.Name)
@Html.TextBoxFor(c => c.Name)

<br />

@Html.LabelFor(c => c.Age)
@Html.TextBoxFor(c => c.Age)

<input type="submit" /> 

</form>
