@model IEnumerable<CatViewModel>


<h1>
    @Model.Count() cats in the system
</h1>

@if (Model.Count() > 0)
{

<ul>

    @foreach (var cat in Model)
    {
        <li>
           @cat.Name (@cat.Age years old) by @cat.Owner 
        </li>
    }

 </ul>
}
else
{
    <div>
        No cats available
    </div>
}
